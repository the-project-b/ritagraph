{
  "name": "@the-project-b/rita-v2-graphs",
  "description": "Rita V2 LangGraph.js graphs for evaluation",
  "version": "2.1.25",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-project-b/ritagraph.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsx --experimental-wasm-modules -r dotenv/config src/index.ts",
    "dev": "langgraphjs dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "langgraph:dev": "langgraphjs dev",
    "langgraph:dev:tunnel": "langgraphjs dev --tunnel",
    "test": "jest",
    "codegen": "echo 'Codegen not needed - using shared package'"
  },
  "author": "Dmitry Zhorov / Andries Verbanck | project-b",
  "license": "MIT",
  "dependencies": {
    "@the-project-b/graphql": "*",
    "@the-project-b/rita-graphs": "*",
    "@langchain/anthropic": "0.3.25",
    "@langchain/community": "0.3.49",
    "@langchain/core": "0.3.66",
    "@langchain/langgraph": "0.4.2",
    "@langchain/langgraph-sdk": "0.0.105",
    "@langchain/mcp-adapters": "0.6.0",
    "@langchain/openai": "0.6.3",
    "@modelcontextprotocol/sdk": "^1.13.0",
    "@the-project-b/graphql": "*",
    "@the-project-b/rita-graphs": "*",
    "graphql": "^16.11.0",
    "langsmith": "^0.3.33",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@langchain/langgraph-cli": "^0.0.56",
    "@jest/globals": "^30.0.0",
    "@tsconfig/recommended": "^1.0.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.32.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "graphql-request": "^7.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.3.4",
    "tsx": "^4.16.2",
    "typescript": "^5.8.3"
  },
  "resolutions": {
    "@langchain/core": "^0.3.66"
  }
}
