{
  "name": "@the-project-b/file-extraction",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src",
    "extract": "tsx src/cli/extract.ts",
    "extract:file": "tsx src/cli/extract-file.ts",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@aws-sdk/client-textract": "^3.709.0",
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/s3-request-presigner": "^3.709.0",
    "@the-project-b/logging": "*",
    "@the-project-b/graphql": "*",
    "@the-project-b/types": "*",
    "archiver": "^7.0.1",
    "dotenv": "^16.5.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "prompts": "^2.4.2",
    "unzipper": "^0.12.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-codegen/typescript": "^4.0.15",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@types/archiver": "^6.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/prompts": "^2.4.9",
    "@types/unzipper": "^0.10.10",
    "jest": "^30.1.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.8.3"
  }
}
