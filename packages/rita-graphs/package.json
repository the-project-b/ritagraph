{
  "name": "@the-project-b/rita-graphs",
  "description": "Shared Rita graphs and tools for LangGraph.js",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "author": "Dmitry Zhorov / Andries Verbanck / Pascal Lohscheidt | project-b",
  "dependencies": {
    "@growthbook/growthbook": "^1.6.1",
    "@langchain/anthropic": "0.3.25",
    "@langchain/community": "0.3.49",
    "@langchain/core": "0.3.72",
    "@langchain/langgraph": "0.4.2",
    "@langchain/langgraph-sdk": "0.0.105",
    "@langchain/mcp-adapters": "0.6.0",
    "@langchain/openai": "0.6.3",
    "@modelcontextprotocol/sdk": "^1.13.0",
    "@the-project-b/graphql": "*",
    "@the-project-b/logging": "*",
    "@the-project-b/prompts": "*",
    "graphql": "^16.11.0",
    "langsmith": "^0.3.63",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-codegen/typescript": "^4.0.15",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@tsconfig/recommended": "^1.0.10",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.32.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "graphql-request": "^7.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  }
}
