{
  "node_version": "20",  
  "image_distro": "wolfi",
  "dockerfile_lines": [
    "ADD ./packages /deps/packages",
    "RUN cd /deps/ritagraph && npm run build --workspace=@the-project-b/projectb-graphql",
    "RUN cd /deps/ritagraph && npm run codegen --workspace=@the-project-b/rita-graphs",
    "RUN cd /deps/ritagraph && npm run build --workspace=@the-project-b/rita-graphs"
  ],
  "dependencies": ["./apps/rita"],
  "graphs": {
    "rita": "./apps/rita/src/graphs/rita/graph.ts:graph"
  },
  "auth": {
    "path": "apps/rita/src/security/auth.ts:auth"
  }
}