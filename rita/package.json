{
  "name": "@the-project-b/rita-v2-graphs",
  "description": "Rita V2 LangGraph.js graphs for evaluation",
  "version": "1.2.0",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-project-b/ritagraph.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "yarn tsc --project tsconfig.json --outDir dist",
    "start": "tsx --experimental-wasm-modules -r dotenv/config src/index.ts",
    "lint": "eslint src",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "langgraph:dev": "npx @langchain/langgraph-cli dev",
    "langgraph:dev:tunnel": "npx @langchain/langgraph-cli dev --tunnel",
    "test": "jest"
  },
  "author": "Dmitry Zhorov / Andries Verbanck | project-b",
  "license": "MIT",
  "dependencies": {
    "@langchain/anthropic": "^0.3.21",
    "@langchain/community": "^0.3.44",
    "@langchain/core": "^0.3.57",
    "@langchain/langgraph": "^0.2.73",
    "@langchain/langgraph-sdk": "^0.0.78",
    "@langchain/mcp-adapters": "^0.4.5",
    "@langchain/openai": "^0.5.11",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "langsmith": "^0.2.15",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.0",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.3.4",
    "tsx": "^3.12.3",
    "typescript": "^5.8.3"
  },
  "resolutions": {
    "@langchain/core": "^0.3.57"
  }
}
